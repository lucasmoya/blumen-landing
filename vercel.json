{
    "builds": [
      { "src": "index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/habitaciones", "dest": "/rooms.html" },
      { "src": "/entorno", "dest": "/entorno.html" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  